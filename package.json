{
  "name": "portalibra",
  "version": "0.0.0",
  "description": "Portal for dictionaries",
  "main": "src/index.ts",
  "scripts": {
    "dev": "set NODE_ENV=development&& nodemon",
    "build": "set NODE_ENV=production",
    "test": "set NODE_ENV=testing&& jest --noStackTrace",
    "lint": "tslint -p ./"
  },
  "pre-commit": "lint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fantkolja/portalibra.git"
  },
  "keywords": [
    "dictionary",
    "portal"
  ],
  "author": "Mykola Fant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fantkolja/portalibra/issues"
  },
  "homepage": "https://github.com/fantkolja/portalibra#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "@types/supertest": "^2.0.6",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "pre-commit": "^1.2.2",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.3",
    "iconv-lite": "^0.4.24"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(test.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
